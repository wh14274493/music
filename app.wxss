/**app.wxss**/
@import "./static/stylesheet.wxss";
/* @import "./static/iconfont.wxss"; */

page{
  height: 100%;
  width: 100%;
  position: fixed;
  top: 0;
  color: #fff;
  background:#222;
}
.container {
  height: 100%;
  width: 100%;
  box-sizing: border-box;
  background: #222;
} 

/* ------------------- */
@font-face {
  font-family: 'iconfont';  /* project id 1127341 */
  src: url('//at.alicdn.com/t/font_1127341_bwv56hz4k9.eot');
  src: url('//at.alicdn.com/t/font_1127341_bwv56hz4k9.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1127341_bwv56hz4k9.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1127341_bwv56hz4k9.woff') format('woff'),
  url('//at.alicdn.com/t/font_1127341_bwv56hz4k9.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1127341_bwv56hz4k9.svg#iconfont') format('svg');
}

.iconfont {
    font-family: "iconfont" !important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon_pl::before {
    content: "\e690";
}
